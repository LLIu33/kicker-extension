{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
     "default_title": "Show info"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js 'unsafe-eval'; object-src 'self'",
  "permissions": [ "http://*/", "https://*/" ]
}